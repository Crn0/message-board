{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "func-names": 0,
        "prefer-arrow-callback": 1,
        "no-unused-vars": ["warn", {
            "argsIgnorePattern": "^_",
            "varsIgnorePattern": "^_"
          }],
          "import/extensions": [
            "error",
            "ignorePackages",
            {
              "js": 0
            }
         ]
    }
}
